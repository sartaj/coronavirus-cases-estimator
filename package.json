{
  "name": "covid-19-cases-estimator",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "dev": "parcel ./src/index.html",
    "build": "parcel build ./src/index.html --public-url /coronavirus-cases-estimator && cp src/united-states-total-potential-coronavirus-cases.png dist/",
    "social-media-image": "cp src/united-states-total-potential-coronavirus-cases.png dist/",
    "predeploy": "rm -rf dist && npm run build && npm run social-media-image",
    "deploy": "gh-pages -d dist",
    "scrape-data": "cd covid19-scraper && node index.js",
    "update": "npm run scrape-data && git commit -am 'update data' && git push && npm run deploy"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/react": "^16.9.32",
    "gh-pages": "^2.2.0",
    "parcel": "^1.12.4",
    "typescript": "^3.8.3"
  },
  "dependencies": {
    "@types/node": "^13.11.1",
    "antd": "^4.1.0",
    "detect-port": "^1.3.0",
    "particles.js": "^2.0.0",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-scroll-parallax": "^2.3.2"
  }
}
